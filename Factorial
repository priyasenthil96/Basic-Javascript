<html>
    <body>
        <script>
            let n = 5; 
            function factorial(n) { 
            let x = 1; 
            for (let i = 1; i <= n; i++) 
            x = x * i; 
            return x; 
            }
            document.write(factorial(n));
        </script>
    </body>
</html>
